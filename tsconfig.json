{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "esModuleInterop": true,
    "paths": {
      "@auth/*": ["src/auth/*"],
      "@auth-facebook/*": ["src/auth-facebook/*"],
      "@auth-google/*": ["src/auth-google/*"],
      "@books/*": ["src/books/*"],
      "@cache/*": ["src/cache/*"],
      "@chat-types/*": ["src/chat-types/*"],
      "@chats/*": ["src/chats/*"],
      "@config/*": ["src/config/*"],
      "@database/*": ["src/database/*"],
      "@fav-stories/*": ["src/fav-stories/*"],
      "@files/*": ["src/files/*"],
      "@home/*": ["src/home/*"],
      "@mail/*": ["src/mail/*"],
      "@mailer/*": ["src/mailer/*"],
      "@roles/*": ["src/roles/*"],
      "@session/*": ["src/session/*"],
      "@stories/*": ["src/stories/*"],
      "@statuses/*": ["src/statuses/*"],
      "@topics/*": ["src/topics/*"],
      "@users/*": ["src/users/*"],
      "@utils/*": ["src/utils/*"],
      "@genders/*": ["src/genders/*"],
      "@story-reviews/*": ["src/story-reviews/*"],
      "@social/*": ["src/social/*"],
      "@tags/*": ["src/tags/*"],
      "@prisma-client/*": ["src/prisma-client/*"],
      "@test/*": ["test/*"],
      "@casl/*": ["src/casl/*"],
      "@time-slots/*": ["src/time-slots/*"],
      "@schedules/*": ["src/schedules/*"],
      "@reading-sessions/*": ["src/reading-sessions/*"],
      "@agora/*": ["src/agora/*"]
    }
  }
}
